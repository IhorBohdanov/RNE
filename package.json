{
  "name": "RNEverywhere",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "webpack serve --mode=development --config ./web/webpack.config.js",
    "web:build": "webpack --mode=production --config ./web/webpack.config.js",
    "chrome:build": "webpack --mode=production --config ./web/webpack.config.js && cp web/manifest.chrome.json web/build/manifest.json && cp web/logo.png web/build/logo.png && cp web/ext_sw.js web/build/ext_sw.js",
    "firefox:build": "webpack --mode=production --config ./web/webpack.config.js && cp web/manifest.firefox.json web/build/manifest.json && cp web/logo.png web/build/logo.png",
    "android:build": "react-native run-android --variant=release",
    "ios:build": "react-native run-ios --configuration Release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "windows": "react-native run-windows",
    "serve:web": "source .env && serve web/build -p $E2E_WEB_SERVE_PORT",
    "serve:web:start": "pm2 start npm --name RNEverywhereWeb -- run serve:web > /dev/null",
    "serve:web:stop": "pm2 stop RNEverywhereWeb > /dev/null",
    "appium:start": "pm2 start \"yarn appium\" --name appium > /dev/null",
    "appium:stop": "pm2 stop appium > /dev/null",
    "e2e:web": "yarn serve:web:start && sleep 3 && E2E_PLATFORM=chrome jest --config jest.config.e2e.js --runInBand && yarn serve:web:stop",
    "e2e:web:extension": "E2E_PLATFORM=chrome-extension jest --config jest.config.e2e.js --runInBand",
    "e2e:ios": "yarn appium:start && sleep 3 && E2E_PLATFORM=ios jest --config jest.config.e2e.js && yarn appium:stop",
    "e2e:ios:sauce": "E2E_PLATFORM=ios E2E_SAUCE=true jest --config jest.config.e2e.js",
    "e2e:android": "yarn appium:start && sleep 3 && E2E_PLATFORM=android jest --config jest.config.e2e.js && yarn appium:stop",
    "e2e:android:sauce": "E2E_PLATFORM=android E2E_SAUCE=true jest --config jest.config.e2e.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.6",
    "react-native-web": "^0.18.9",
    "react-native-windows": "0.71.5",
    "react-router-native": "^6.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "appium": "^2.0.0",
    "appium-doctor": "^1.16.2",
    "appium-uiautomator2-driver": "^2.29.2",
    "appium-xcuitest-driver": "^4.32.19",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-react-native-web": "^0.18.9",
    "dotenv": "^16.0.3",
    "eslint": "^8.19.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.1",
    "jest-environment-node": "^29.5.0",
    "metro-config": "^0.73.9",
    "metro-react-native-babel-preset": "0.73.9",
    "pm2": "^5.3.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "serve": "^14.2.0",
    "typescript": "4.8.4",
    "url-loader": "^4.1.1",
    "webdriverio": "^8.10.5",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
